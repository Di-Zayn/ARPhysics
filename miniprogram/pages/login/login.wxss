page{
  height: 100%;
  background-color: #F5F5F5;
  align-items: center;
  justify-content: center;
}
.logo{
  width: 46%;
  margin-bottom: 180rpx;
}
form{
  width: 100%;
}
input{
  padding-left: 10%;
  display: block;
  margin: 0 auto;
}
.account{
  background: url(data:image/png;base64,) no-repeat 20rpx center;
  background-size: auto 40rpx;
}
.password{
  margin: 20rpx auto 80rpx;
  background: url(data:image/png;base64,) no-repeat 26rpx center;
  background-size: auto 34rpx;
}
button.btn{
  background: #006FAD!important;
  color: #fff!important;
  width: 80%!important;
  border-radius: 40rpx;
}
button.btn.disabled{
  background: #ddd!important;
  color: #999!important;
}