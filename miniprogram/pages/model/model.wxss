page {
  height: 100%;
}
.entry {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.entry button {
  width: 400rpx;
  height: 72rpx;
  background-color: #fff;
  font-weight: normal;
  color: #333;
  border: 1px solid #ddd;
  margin-bottom: 80rpx;
  font-size: 28rpx;
}
.content {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.bar {
  width: 300px;
  margin: 360px auto 0;
}
.bar .slider-item {
  margin-top: 10px;
  margin-bottom: 10px;
  display: flex;
  border-bottom: 1px dashed #ddd;
  align-items: center;
}
.bar .slider-item:last-child {
  border: 0;
}
.bar .slider-item text {
  font-size: 10px;
  color: #333;
  margin-right: 16px;
  line-height: 20px;
  /* vertical-align: middle; */
}
.bar .slider-item slider {
  flex: 1;
}
.sup-text {
  font-weight: normal;
  font-size: 20rpx;
  color: #333;
  margin-left: -16px;
}
.bar .slider-item .lastText {
  margin-left: -7px;
}
.content icon {
  position: absolute;
  top: 0px;
  right: 10px;
  width: 5%;
  height: 5%;
}
.canvas {
  position: absolute;
  top: 22px;
  width: 98%;
  height: 300px;
  border: 1px solid #ddd;
}
