@keyframes rotateForever {
  from {
    transform: rotate(0);
  }
  to {
    transform: rotate(360deg);
  }
}
camera {
  position: absolute;
  width: 100vw;
  height: 100vh;
  z-index: 10;
}

#capture {
  position: absolute;
  left: 750rpx;
}

#overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(63, 63, 63, 0.6);
  z-index: 30;
}

#overlay-top {
  position: absolute;
  margin: 60rpx 44rpx 0 54rpx;
  left: 0;
  right: 0;
}

#title {
  position: absolute;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC, serif;
  font-weight: 500;
  color: #ffffff;
  line-height: 46rpx;
}

#logo {
  position: absolute;
  top: 0;
  right: 0;
  width: 178rpx;
  height: 54rpx;
}

#desc {
  position: absolute;
  top: 106rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC, serif;
  font-weight: 500;
  color: #ffffff;
  line-height: 46rpx;
}

#overlay-bottom {
  position: absolute;
  width: 540rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
}

#loading {
  position: absolute;
  width: fit-content;
  height: fit-content;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  z-index: 40;
}

.primary-button {
  width: 540rpx;
  height: 112rpx;
  margin-top: 22rpx;
  text-align: center;

  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC, serif;
  font-weight: 500;
  color: #ffffff;
  line-height: 112rpx;
}

.secondary-button {
  width: 540rpx;
  height: 80rpx;
  margin-top: 22rpx;
  text-align: center;

  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC, serif;
  font-weight: 500;
  color: #ededed;
  line-height: 80rpx;
}

#content {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  margin: auto;
  width: fit-content;
  height: fit-content;
  z-index: 20;
}

#select {
  position: absolute;
  bottom: 210rpx;
  right: 0;
  z-index: 20;
}
.select {
  width: 180rpx;
  height: 88rpx;
  color: white;
  line-height: 88rpx;
  text-align: center;
  margin-top: 20rpx;
  border-top-left-radius: 10rpx;
  border-bottom-left-radius: 10rpx;

  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC, serif;
  font-weight: 500;

  box-shadow: 0 4rpx 8rpx 0 rgba(0, 0, 0, 0.5);
  background: linear-gradient(90deg, #eea167 0%, #f97605 50%, #eea167 100%);
}
.selected {
  color: black;
  background: linear-gradient(90deg, #ababab 0%, #d6d6d6 50%, #ababab 100%);
}
.contents {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  display: flex;
  justify-content: center;
  align-items: center;
}
.ar_entry {
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.ar_entry video {
  display: block;
  width: 500rpx;
  height: 156rpx;
  margin-bottom: 40rpx;
}
.ar_entry image {
  width: 300rpx;
  margin-top: 20rpx;
}
.ar_entry text {
  display: block;
  width: 500rpx;
  margin-bottom: 40rpx;
}
.ar_main {
  width: 100%;
}
.model_vanvas {
  width: 300px;
  height: 500px;
  margin: 0 auto;
}
#model1{
  position: absolute;
  margin: -40vh auto 0 auto;
  width:100vw;
  height:60vh;
  text-align:center;
  z-index: 90;  
}
#models{
  position: absolute;
  margin: -40vh auto 0 auto;
  width:100%;
  text-align:center;
  z-index: 90;  
}
.btns {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-end;
  z-index:96;
}
.btns image {
  width: 250rpx;
  z-index:96;
  /* margin-bottom: 20rpx; */
}
.ar_main .model {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.ar_main .model.both canvas {
  width: 50%;
  height: 100vw;
}
.video4 video {
  width: 40%;
  height: 26.7vw;
}
#loaderNeed {
  width: 10px;
  height: 10px;
  position: fixed;
  top: -10px;
  left: -10px;
}
.full-page {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99999;
  background-color: #fff;
}
.scan_loading {
  background-color: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
}
.scan_loading image {
  width: 600rpx;
  height: 600rpx;
  animation: rotateForever 3s linear infinite;
}
.video4-main {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.video4-main video {
  width: 700rpx;
  margin-left:20rpx;
}
.video4-main text {
  margin-left: 20rpx;
}

